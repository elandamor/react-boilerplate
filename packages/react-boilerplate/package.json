{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "A highly scalable, offline-first foundation with the best developer experience and a focus on performance and best practices.",
  "repository": {
    "type": "git",
    "url": "git://github.com/elandamor/react-boilerplate.git"
  },
  "resolutions": {
    "@types/node": "8.10.28",
    "@types/react": "16.4.14",
    "@types/react-router-dom": "4.3.1"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8"
  },
  "author": "Thandolwethu Mpofu",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.js && node internals/scripts/minifyServiceWorker.js",
    "build:dll": "cross-env NODE_ENV=development node internals/scripts/dependencies.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint": "npm run lint:ts",
    "lint:ts": "npm run lint:tslint",
    "lint:tslint": "tslint --project tsconfig.json --config tslint.json",
    "postinstall": "npm run build:dll",
    "precommit": "lint-staged",
    "pretest": "npm run lint",
    "start": "cross-env NODE_ENV=development node server",
    "start:prod": "cross-env NODE_ENV=production node server",
    "test": "cross-env NODE_ENV=test jest --coverage",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "dllPlugin": {
    "path": "node_modules/react-boilerplate-dlls",
    "exclude": [
      "chalk",
      "compression",
      "cross-env",
      "express",
      "ip",
      "minimist",
      "sanitize.css"
    ],
    "include": [
      "core-js",
      "lodash",
      "eventsource-polyfill"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "app/**/*.{ts,tsx}",
      "!app/**/*.spec.{ts,tsx}",
      "!app/app.ts",
      "!app/global-styles.ts",
      "!app/*/*/Loadable.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 98,
        "branches": 91,
        "functions": 98,
        "lines": 98
      }
    },
    "roots": [
      "<rootDir>/app"
    ],
    "transform": {
      "^.+\\.css$": "<rootDir>/internals/testing/cssTransform.js",
      ".+\\.tsx?$": "ts-jest",
      "^(?!.*\\.(css|json)$)": "<rootDir>/internals/testing/fileTransform.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/enzyme-setup.ts",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/internals/testing/polyfills.ts"
    ]
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "apollo-cache-inmemory": "1.2.10",
    "apollo-cache-persist": "0.1.1",
    "apollo-client": "2.4.2",
    "apollo-link": "1.2.3",
    "apollo-link-batch-http": "1.2.3",
    "apollo-link-error": "1.1.1",
    "apollo-link-retry": "2.2.5",
    "apollo-link-state": "0.4.2",
    "apollo-link-ws": "1.0.9",
    "apollo-utilities": "1.0.21",
    "bugsnag-js": "4.7.3",
    "bugsnag-react": "1.1.1",
    "classnames": "2.2.6",
    "fontfaceobserver": "2.0.13",
    "graphql": "14.0.2",
    "graphql-tag": "2.9.2",
    "history": "4.7.2",
    "hoist-non-react-statics": "3.0.1",
    "intl": "1.2.5",
    "localforage": "1.7.2",
    "no-scroll": "2.1.1",
    "react": "16.5.2",
    "react-apollo": "2.1.11",
    "react-dom": "16.5.2",
    "react-helmet": "5.2.0",
    "react-loadable": "5.5.0",
    "react-measure": "2.1.2",
    "react-router-dom": "4.3.1",
    "react-select": "2.0.0",
    "react-slick": "0.23.1",
    "react-spring": "5.8.0",
    "react-transition-group": "2.4.0",
    "react-virtualized": "9.20.1",
    "rxjs": "6.3.2",
    "sanitize.css": "7.0.3",
    "styled-components": "3.4.9",
    "subscriptions-transport-ws": "0.9.15",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@storybook/cli": "3.4.11",
    "@types/classnames": "2.2.6",
    "@types/debug": "0.0.30",
    "@types/enzyme": "3.1.14",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/fontfaceobserver": "0.0.6",
    "@types/graphql": "14.0.0",
    "@types/intl": "1.2.0",
    "@types/jest": "23.3.2",
    "@types/lodash": "4.14.116",
    "@types/react": "16.4.14",
    "@types/react-dom": "16.0.7",
    "@types/react-helmet": "5.0.7",
    "@types/react-loadable": "5.4.1",
    "@types/react-measure": "2.0.4",
    "@types/react-router-dom": "4.3.1",
    "@types/react-slick": "0.23.2",
    "@types/react-test-renderer": "16.0.2",
    "@types/webpack-env": "1.13.6",
    "@types/ws": "6.0.1",
    "add-asset-html-webpack-plugin": "3.0.1",
    "chalk": "2.4.1",
    "circular-dependency-plugin": "5.0.2",
    "clean-webpack-plugin": "0.1.19",
    "compression": "1.7.3",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "css-loader": "1.0.0",
    "debug": "4.0.1",
    "dotenv-webpack": "1.5.7",
    "enzyme": "3.6.0",
    "enzyme-adapter-react-16": "1.5.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.6.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-import-resolver-webpack": "0.10.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "2.6.2",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "0.7.0",
    "express": "4.16.3",
    "file-loader": "2.0.0",
    "fork-ts-checker-webpack-plugin": "0.4.9",
    "glob": "7.1.3",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "0.14.3",
    "image-webpack-loader": "4.3.1",
    "ip": "1.1.5",
    "jest": "23.6.0",
    "jest-styled-components": "6.2.1",
    "json-loader": "0.5.7",
    "lint-staged": "7.3.0",
    "lodash": "4.17.11",
    "minimist": "1.2.0",
    "ngrok": "3.0.1",
    "node-plop": "0.16.0",
    "offline-plugin": "5.0.5",
    "plop": "2.1.0",
    "prettier": "1.14.3",
    "raf": "3.4.0",
    "react-test-renderer": "16.5.2",
    "source-map-loader": "0.2.4",
    "style-loader": "0.23.0",
    "stylelint": "9.5.0",
