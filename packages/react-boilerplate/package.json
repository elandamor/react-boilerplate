    "tslint-config-prettier": "1.15.0",
    "typescript": "3.0.3",
    "uglify-js": "3.4.9",
    "url-loader": "1.1.1",
    "webpack": "4.19.1",
    "webpack-bundle-analyzer": "3.0.2",
