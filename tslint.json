{
    "defaultSeverity": "error",
    "extends": [
      "tslint-config-airbnb",
      "tslint-config-prettier"
    ],
    "jsRules": {},
    "rules": {
      "import-name": false,
      "ordered-imports": [
        true,
        {
          "import-sources-order": "any"
        }
      ],
      "quotemark": [true, "single", "jsx-double"],
      "ter-arrow-parens": [true, "always"],
      "variable-name": [true, "ban-keywords", "allow-pascal-case"]
    },
    "rulesDirectory": []
}
