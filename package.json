    "react-dom": "16.6.0",
    "react-helmet": "5.2.0",
    "react-measure": "2.1.2",
    "react-router-dom": "4.3.1",
    "sanitize.css": "8.0.0",
    "styled-components": "4.0.2",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "pd-react-scripts": "0.0.13"
